$group

	h5 Relations
	small
		table
			class pretty-table-2
			thead
				tr
					th Name
					th Role
					th Cardinality
					th Inherited
					th Flags
					th Description
			tbody
				$foreach relation in element.relations
					tr
						td ${relation.name}
						td ${relation.role}
						td ${relation.cardinality}
						td ${relation.inheritedFrom}
						td ${relation.flags}
						td
							$foreach comment in relation.comments
								span ${comment} &nbsp;


